<div class="container">
  <h4>注文履歴一覧</h4>
  <table class="table">
    <thead>
      <tr>
        <th>購入日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
    </thead>
    <% @order_details.each do |order_detail|%>
    <tbody>
      <tr>
        <td><%= order_detail.created_at.strftime('%Y/%m/%d')%></td>
        <td></td>
        <td><%= order_detail.customer.name %></td>
      </tr>
    </tbody>
    <% end %>
  </table>
</div>