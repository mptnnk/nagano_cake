<div class="border border-secondary rounded">
 <table class="table table-borderless">
   <tr>
     <th class="table-active">ジャンル検索</th>
   </tr>
   <% genres.each do |genre|%>
   <tr>
     <td>
       <%= link_to items_path(genre_id: genre.id),style:"color:black" do %>
         <%= genre.name %>
       <% end %>
     </td>
   </tr>
   <% end %>
 </table>
</div>
